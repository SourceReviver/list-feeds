{
  "author": "igorlogius",
  "description": "list available feeds (rss,atom,json)",
  "icons": {
    "145": "icon.png"
  },
  "background": {
	"scripts": ["background.js"]
  },
  "manifest_version": 2,
  "name": "list-feeds",
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html"
  },
  "page_action": {
    "browser_style": true,
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "version": "1.7.6"
}
